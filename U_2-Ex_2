package unt_2_ex_1;
abstract class Bike
{
	Bike()
	{
	System.out.println("bike is created");	
	}
	abstract void run();
	void changeGear()
	{
		System.out.println("gear changed");
	}
}
class Honda extends Bike
{
	void run()
	{
		System.out.println("running safely..");
	}
}
public class AbstractMain {
 public static void main(String args[])
 {
	 Bike obj = new Honda();
	 obj.run();
	 obj.changeGear();
 }
}

OUTPUT:

bike is created
running safely..
gear changed 
